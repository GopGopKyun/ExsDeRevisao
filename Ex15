#include <stdio.h>

int main()
{

    int a, i, soma, n;
    
    printf("Insira o numero desejado: ");
    scanf("%d", &a);
    
    for (i=1;i<a;i++){
        if (a % i == 0){
            soma += i;
            n++;
        }
    }
    printf("A soma dos divisores sera: %d", soma);
    return 0;
}

